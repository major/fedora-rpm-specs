Provides:       tex(fithesis-czech.def) = %{tl_version}
Provides:       tex(fithesis-czech.def) = %{tl_version}
Provides:       tex(fithesis-czech.def) = %{tl_version}
Provides:       tex(fithesis-czech.def) = %{tl_version}
Provides:       tex(fithesis-czech.def) = %{tl_version}
Provides:       tex(fithesis-czech.def) = %{tl_version}
Provides:       tex(fithesis-czech.def) = %{tl_version}
